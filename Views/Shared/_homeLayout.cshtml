
<html  lang="en-US">
   <head>
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1">
      <meta name="description" content="Coding Tutorials with examples">
      <meta name="author" content="Alec Sears">
      <title>@ViewData["controller"] - @ViewData["Title"]</title>
      
      <!-- Bootstrap core CSS -->
      <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
      
      <!-- Site CSS -->
      <link rel="stylesheet" href="~/css/style.css" >
      <link rel="stylesheet" href="~/css/homeLayout_CSS.css" asp-append-version="true" />
            <!-- Sidebar CSS -->
      <link rel="stylesheet" href="~/css/sidebars.css" >
      
      <!-- favicon -->
      <link rel="apple-touch-icon" sizes="180x180" href="~/lib/apple-touch-icon.png">
      <link rel="icon" type="image/png" sizes="32x32" href="~/lib/favicon-32x32.png">
      <link rel="icon" type="image/png" sizes="16x16" href="~/lib/favicon-16x16.png">
      <link rel="manifest" href="~/lib/site.webmanifest">
      
      <!-- Google Fonts -->
      <link rel="preconnect" href="https://fonts.googleapis.com">
      <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
      <link
         href="https://fonts.googleapis.com/css2?family=Dancing+Script:wght@700&family=Lato:wght@400;700&family=Lexend:wght@200&family=Poppins&family=UnifrakturCook&family=UnifrakturMaguntia&display=swap"
         rel="stylesheet">
      
      <!-- Prism CSS -->
      <link rel="stylesheet" href="~/css/prism.css" asp-append-version="true" />
      <!-- Google Icons -->
      <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
      <!-- Custom styles sidebar -->
      
   </head>
   
   <body id="bodyCapture">   
      <div class="pre-nav bg-white text-muted text-center">
         <div class="fs-2 p-3">
            <span class="border-bottom border-muted"><span class="text-dark">A</span>lec <span class="text-dark">S</span>cripts</span>
         </div>
      </div>
      @* Navbar *@
           <nav style="" class="thenavbar py-0 my-0 shadow-sm navbar  sticky-top navbar-expand-lg bg-white ">
         <style>
            .material-symbols-outlined{
            color: black;
            }
         </style>
         <div class="container-fluid">
            <a  class="text-decoration-none " asp-action="index" asp-controller="home" >
               <img src="/lib/favicon-32x32.png">
            </a>
            <button class=" navbar-toggler" type="button" data-bs-toggle="collapse"
               data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
               aria-label="Toggle navigation">
                  <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" fill="currentColor" class="bi bi-list" viewBox="0 0 16 16">
                     <path fill-rule="evenodd" d="M2.5 12a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z"/>
                  </svg>
            </button>
            <div class=" collapse navbar-collapse " id="navbarSupportedContent">
               <ul class=" navbar-nav mx-auto mb-2 mb-lg-0 ">
                  <li class="nav-item">
                     <a class="nav-link" asp-controller="home" asp-action="Directory">Directory</a>
                  </li>
                  <li class="nav-item">
                     <a class="nav-link" asp-controller="Glossary" asp-action="index">Glossary</a>
                  </li>
                  <li class="nav-item">
                     @* <a role="button" onclick="changeToDarkMode()" class="nav-link" >Dark Mode</a> *@
                  </li>
                  @* 
                  <li class="nav-item dropdown">
                     <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown"
                        aria-expanded="false">
                     Python
                     </a>
                     <ul class="dropdown-menu">
                        <li><a class=" dropdown-item" asp-controller="Python" asp-action="Index"> Home </a></li>
                        <li><a class=" dropdown-item" asp-controller="Python" asp-action="Abs"> Abs </a></li>
                        <li><a class=" dropdown-item" asp-controller="Python" asp-action="Casting"> Casting </a></li>
                        <li>
                           <hr class="dropdown-divider">
                        </li>
                        *@
                     </ul>
                  </li>
               </ul>
               @* 
               <form class="d-flex" role="search">
                  <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                  <button class="btn btn-outline-success" type="submit">Search</button>
               </form>
               *@
            </div>
         </div>
      </nav>
      <div class="divider"> </div>
      <div class="mainBody">
         @RenderBody()
      </div>
   </body>
