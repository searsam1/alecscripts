
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <!-- Bootstrap core CSS -->
    <link href="~/lib/bootstrap/bootstrap-5.0.2-dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/sidebars.css" rel="stylesheet">

    @* favicon Recommended Settings *@
    <link rel="apple-touch-icon" sizes="180x180" href="~/lib/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/lib/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/lib/favicon-16x16.png">
    <link rel="manifest" href="~/lib/site.webmanifest">

    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MvcAlecScripts.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/alecScriptsStyles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />

    @* For /alecscripts/Views/Sass/SassVariablesCSS.css *@
    <link rel="stylesheet" href="Views/Sass/SassVariablesCSS.css" />

    @* Google fonts *@
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Dancing+Script:wght@700&family=Lato:wght@400;700&family=Lexend:wght@200&family=Poppins&family=UnifrakturCook&family=UnifrakturMaguntia&display=swap"
        rel="stylesheet">

    @* /Prism CSS *@
    <link rel="stylesheet" href="~/css/prism.css" asp-append-version="true" />

    @* Google Icons *@
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
    <title>@ViewData["title"]</title>
</head>
<body>
    @RenderBody()
</body>
<script src="/js/textAreaTab.js"></script>
</html>